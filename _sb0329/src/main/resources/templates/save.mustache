{{>layout/header}}

<h1>게시글 등록</h1>

<div class="col-md-12">
    <div class="col-md-4">
   <form method="post" action="/save">
    <div class="form-group">
        <label for="title">제목</label>
        <input type="text" class="form-control" name="prodId" placeholder="제목을 입력하세요">
    </div>
    <div class="form-group">
        <label for="author">작성자</label>
        <input type="text" class="form-control" name="prodName" placeholder="작성자를 입력하세요">
    </div>
    <div class="form-group">
        <label for="content">내용</label>
        <textarea class="form-control" name="prodPrice" placeholder="내용을 입력하세요"></textarea>
    </div>
  <a href="/" role="button" class="btn btn-secondary">취소</a>
    <button type="submit" class="btn btn-primary">등록</button>
</form>
    </div>
</div>

{{>layout/footer}}